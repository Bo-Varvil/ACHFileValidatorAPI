<beanio xmlns="http://www.beanio.org/2012/03"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">
    
    <stream name="ach" format="fixedlength">
        <record name="fileHeader" class="com.groupg.achfilevalidator.models.FileHeader" occurs="1">
            <field name="recordCode" length="1" minOccurs="1" maxOccurs="1"/>
            <field name="priorityCode" length="2" minOccurs="1" maxOccurs="1"/>
            <field name="immediateDestination" length="10" minOccurs="1" maxOccurs="1"/>
            <field name="immediateOrigin" length="10" minOccurs="1" maxOccurs="1"/>
            <field name="fileCreationDate" length="6" minOccurs="0" maxOccurs="1"/>
            <field name="fileCreationTime" length="4" minOccurs="1" maxOccurs="1"/>
            <field name="fileIdModifier" length="1" minOccurs="1" maxOccurs="1"/>
            <field name="recordSize" length="3" minOccurs="1" maxOccurs="1"/>
            <field name="blockingFactor" length="2" minOccurs="1" maxOccurs="1"/>
            <field name="formatCode" length="1" minOccurs="1" maxOccurs="1"/>
            <field name="immediateDestinationName" length="23" minOccurs="0" maxOccurs="1"/>
            <field name="immediateOriginName" length="23" minOccurs="0" maxOccurs="1"/>
            <field name="referenceCode" length="8" minOccurs="0" maxOccurs="1"/>
        </record>
        <record name="batchHeader" class="" occurs="1">
            <field name="recordCode" length="1" minOccurs="1"/>
            <field name="serviceCode" length="3" minOccurs="1"/>
            <field name="companyName" length="16" minOccurs="1"/>
            <field name="companyData" length="20" minOccurs="0"/>
            <field name="companyIdentification" length="10" minOccurs="1"/>
            <field name="standardEntryClass" length="3" minOccurs="1"/>
            <field name="companyEntryDescription" length="10" minOccurs="1"/>
            <field name="companyDescriptiveDate" length="6" minOccurs="0"/>
            <field name="effectiveEntryDate" length="6" minOccurs="1"/>
            <field name="settlementDate" length="3" minOccurs="0"/>
            <field name="originatorCode" length="1" minOccurs="1"/>
            <field name="originatingDFI" length="8" minOccurs="1"/>
            <field name="batchNumber" length="7" minOccurs="1"/>
        </record>
    </stream>
</beanio>